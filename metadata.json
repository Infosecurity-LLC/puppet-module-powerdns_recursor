{
  "name": "prozeta-powerdns_recursor",
  "version": "0.0.1",
  "author": "jkaufman",
  "summary": "Module for managing PowerDNS recursor.",
  "license": "Apache-2.0",
  "source": "https://github.com/prozeta/puppet-powerdns_recursor",
  "project_page": "https://github.com/prozeta/puppet-powerdns_recursor",
  "issues_url": "https://github.com/prozeta/puppet-powerdns_recursor/issues",
  "requirements": [
    {
      "name": "puppet",
